# Quickly change to frequently used directories

local -A aliases
aliases=(
    d        "$PROJECTS_DIR"
    df       "$DOTFILES_DIR"
    p        "$PROJECTS_DIR/power"
    nw       "$PROJECTS_DIR/power/nitro-web"
    db       "$HOME/Dropbox"
    sbl      "$HOME/Dropbox/Bass Archives/ScottBassLessons"
)

cd ${aliases[${1:-d}]}
pwd
