# Quickly change to frequently used directories

local -A aliases
aliases=(
    d        "$PROJECTS_DIR"
    df       "$DOTFILES_DIR"
    mc       "$PROJECTS_DIR/mediacomments"
    fcc      "$PROJECTS_DIR/freecodecamp"
    p        "$PROJECTS_DIR/power"
    pw       "$PROJECTS_DIR/power/nitro-web"
)

cd ${aliases[${1:-d}]}
pwd
